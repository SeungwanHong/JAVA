package testBoard.interDB.impl;

import java.sql.Connection;
import java.sql.PreparedStatement;
import java.sql.ResultSet;
import java.sql.SQLException;

import testBoard.interDB.SQLSend;

public class SQLSender implements SQLSend{

	
	private ResultSet rs = null;
	
	public void testsend(String sql, Connection conn, PreparedStatement pstmt) {
		sql ="SELECT * FROM EMP";
		pstmt = conn.prepareStatement(sql);
	
		rs = pstmt.executeQuery();
	
		while(rs.next()){
			String empno = rs.getString("EMPNO");
			String ename = rs.getString("ENAME");
			int sal = rs.getInt("SAL");
			System.out.println("사번  : "+empno+" 이름 : "+ename+" 급여 : "+sal);
	
		}
	}
}
