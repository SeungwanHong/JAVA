package hsw.example;

public class mai01 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for(char ch = 'A' ; ch <= 'Z' ; ch++){
			System.out.println("대문자 : "+ch);
		}
		for(char ch = 'a' ; ch <= 'z' ; ch++){
			System.out.println("\t\t소문자 : "+ch);
		}
		for(char ch = 'ㄱ' ; ch <= 'ㅎ' ; ch++){
			System.out.println("한글 : "+ch);
		}
	}

}
