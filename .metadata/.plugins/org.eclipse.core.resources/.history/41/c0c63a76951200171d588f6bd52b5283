import java.io.File;
import java.io.FileReader;
import java.io.Reader;

public class main02 {
	public static void main(String[] args) {
		File file = new File("test1.txt");
		
		//
		if(file.exists()){
			Reader in = new FileReader(file)
		}else{
			System.out.println("파일이 없습니다.");
		}
		System.out.println("프로그램 종료");
	}
}
